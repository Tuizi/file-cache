{
  "name": "@tuizi/file-cache",
  "version": "1.0.0",
  "description": "A simple file cache system for Node.js. The cache is stored in memory and on the filesystem, allowing for fast access and persistence between runs.",
  "main": "dist/index.js",
  "engines": {
    "node": ">=19.2.0"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tuizi/filecache.git"
  },
  "author": "Tuizi",
  "bugs": {
    "url": "https://github.com/Tuizi/filecache/issues"
  },
  "homepage": "https://github.com/Tuizi/filecache#readme",
  "keywords": [
    "file cache",
    "in-memory cache",
    "persistent cache",
    "Node.js cache",
    "cache",
    "storage"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "jest": "29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}